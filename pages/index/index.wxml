<view class='flex_f height_100'>

  <view class='flex_1'>
    <scroll-view class='body'>
      <image style="width: 100%; height: 100%" src="{{home_background_url}}"></image>
      <view class='note' bindtap='noteHandle'>
        <image style="width: 100%; height: 100%" src="{{home_notes_url}}"></image>
      </view>

      <view class='start' bindtap='startHandle'>
        <image style="width: 100%; height: 100%" src="{{home_start_url}}"></image>
      </view>

      <import src="../../template/note/note" />
      <template is="note" data="{{display,height,width,left,top,notes_background_url,notes_button,notes_info}}" />

    </scroll-view>

  </view>

  <import src="../../template/navBar/navBar" />
  <template is="navBar" />

</view>